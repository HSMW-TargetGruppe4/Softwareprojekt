ection/profanity_en.csv')\n\ndef hate_score_tokens(text):\n    dictionary = hate_dict['canonical_form_1']\n    score = 0\n   \n    for word in text:\n        if dictionary.eq(word).any():\n            index= int(hate_dict.index[hate_dict['canonical_form_1'] == word][0])\n            score = score + hate_dict['severity_rating'].iloc[index]\n            \n    return score\n\n# %% [code] {\"execution\":{\"iopub.status.busy\":\"2022-12-13T17:23:21.036687Z\",\"iopub.execute_input\":\"2022-12-13T17:23:21.037250Z\",\"iopub.status.idle\":\"2022-12-13T17:23:24.500545Z\",\"shell.execute_reply.started\":\"2022-12-13T17:23:21.037214Z\",\"shell.execute_reply\":\"2022-12-13T17:23:24.499825Z\"}}\ndf['c_text_tokens_EN'] = df['text_EN'].apply(tokenizer.tokenize)\n\n# %% [code] {\"execution\":{\"iopub.status.busy\":\"2022-12-13T17:23:24.501443Z\",\"iopub.execute_input\":\"2022-12-13T17:23:24.502327Z\",\"iopub.status.idle\":\"2022-12-13T17:27:14.784824Z\",\"shell.execute_reply.started\":\"2022-12-13T17:23:24.502300Z\",\"shell.execute_reply\":\"2022-12-13T17:27:14.783010Z\"}}\ndf['profanity_score'] = df['c_text_tokens_EN'].progress_apply(lambda n : hate_score_tokens(n))\n\n# %% [markdown]\n# # Feature Engineering\n\n# %% [code] {\"papermill\":{\"duration\":0.077536,\"end_time\":\"2022-11-22T10:44:40.500564\",\"exception\":false,\"start_time\":\"2022-11-22T10:44:40.423028\",\"status\":\"completed\"},\"tags\":[],\"execution\":{\"iopub.status.busy\":\"2022-12-13T17:27:14.786340Z\",\"iopub.execute_input\":\"2022-12-13T17:27:14.786725Z\",\"iopub.status.idle\":\"2022-12-13T17:27:14.793028Z\",\"shell.execute_reply.started\":\"2022-12-13T17:27:14.786692Z\",\"shell.execute_reply\":\"2022-12-13T17:27:14.791791Z\"}}\n#split the data into input X and output y\nX = df['c_text_tokens']\ny = df['hatespeech']\n\n# %% [code] {\"execution\":{\"iopub.status.busy\":\"2022-12-13T17:27:14.794737Z\",\"iopub.execute_input\":\"2022-12-13T17:27:14.795103Z\",\"iopub.status.idle\":\"2022-12-13T17:27:15.490346Z\",